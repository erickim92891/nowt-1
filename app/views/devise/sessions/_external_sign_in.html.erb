<ul class="list-inline">
  <li><%= link_to user_omniauth_authorize_path(:facebook), {"title" => "Sign in with Facebook", "data-toogle" => "tooltip", "data-placement" => "bottom", "class" => "tooltips"} do %>
    <i class="fa fa-facebook-square fa-2x"></i>
  <% end %>
  <li><%= link_to user_omniauth_authorize_path(:twitter), {"title" => "Sign in with Twitter", "data-toogle" => "tooltip", "data-placement" => "bottom", "class" => "tooltips"} do %>
    <i class="fa fa-twitter-square fa-2x"></i>
  <% end %>
  <li><%= link_to user_omniauth_authorize_path(:google_oauth2), {"title" => "Sign in with Google+", "data-toogle" => "tooltip", "data-placement" => "bottom", "class" => "tooltips"} do %>
    <i class="fa fa-google-plus-square fa-2x"></i>
  <% end %>
</ul>

